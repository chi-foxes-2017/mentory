<div class="row">
  <div class="col s6 push-s1">
    <h1>Available Pairing Sessions</h1>
  </div>
</div>

<div class="row">
  <div class="col s6 push-s1">
    <ul>
      <%= render partial: "pairings/offered_pairing", collection: @offered_pairings %>
    </ul>
  </div>

  <div class="col s3 right pull-s1">
    <div class="card index-form-card">
      <% if logged_in? %>
        <%= render "pairings/new" %>
      <% else %>
        <h5> Once you log in you'll be able to access the pairing functionalities</h5>
      <% end %>
    </div>
  </div>
</div>
